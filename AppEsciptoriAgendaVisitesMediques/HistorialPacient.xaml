<Page
    x:Class="AppEsciptoriAgendaVisitesMediques.HistorialPacient"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AppEsciptoriAgendaVisitesMediques"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="SkyBlue">
        <TextBlock x:Name="nifText" Text="Visualizador de informes"  FontSize="40" FontWeight="Bold" HorizontalAlignment="Center" Margin="20,16,20,20"/>
        <WebView x:Name="webViewJaper" Margin="100, 100, 100, 100" />
        <Button x:Name="btnback" Background="Navy" Content="Torna" VerticalAlignment="Bottom"  Click="torna_Click" HorizontalAlignment="Center" Margin="20, 20, 20, 20">
            <Button.Foreground>
                <SolidColorBrush Color="White"/>
            </Button.Foreground>
            <Button.CornerRadius>
                <CornerRadius>5</CornerRadius>
            </Button.CornerRadius>
        </Button>
    </Grid>
</Page>
